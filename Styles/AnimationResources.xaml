<ResourceDictionary
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

	<SineEase
		x:Key="SinEaseIn"
		EasingMode="EaseIn" />

	<SineEase
		x:Key="SinEaseOut"
		EasingMode="EaseOut" />

	<SineEase
		x:Key="SinEaseInOut"
		EasingMode="EaseInOut" />

	<ExponentialEase
		x:Key="ExponentailEaseIn"
		EasingMode="EaseIn" />

	<ExponentialEase
		x:Key="ExponentailEaseOut"
		EasingMode="EaseOut" />

	<ExponentialEase
		x:Key="ExponentailEaseInOut"
		EasingMode="EaseInOut" />

</ResourceDictionary>