<ResourceDictionary
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

	<SolidColorBrush
		x:Key="ForegroundBrush"
		Color="#DDFFFFFF" />

	<SolidColorBrush
		x:Key="ForegroundLightBrush"
		Color="#89FFFFFF" />

	<SolidColorBrush
		x:Key="BackgroundBrush"
		Color="#FF222222" />

	<SolidColorBrush
		x:Key="BackgroundLightBrush"
		Color="#FF333333" />

	<SolidColorBrush
		x:Key="TrimBrush"
		Color="DeepPink" />

	<SolidColorBrush
		x:Key="ControlBackgroundBrush"
		Color="#1A1A1A" />

	<SolidColorBrush
		x:Key="ControlHighlightBrush"
		Color="#11FFFFFF" />

</ResourceDictionary>